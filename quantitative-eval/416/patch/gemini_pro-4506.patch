     int subtract_matrices(int a[3][3], int b[3][3], int c[3][3])
     {
         for (int i = 0; i < 3; i++) {
             for (int j = 0; j < 3; j++) {
                 c[i][j] = a[i][j] - b[i][j];
             }
         }
     }
     
     int multiply_matrices(int a[3][3], int b[3][3], int c[3][3])
     {
         for (int i = 0; i < 3; i++) {
             for (int j = 0; j < 3; j++) {
                 c[i][j] = 0;
                 for (int k = 0; k < 3; k++) {
                     c[i][j] += a[i][k] * b[k][j];
                 }
             }
         }
     }
     
